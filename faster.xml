<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="faster"
            revision="8.0"
            fetch="https://github.com/FasterRom-Staging/" />

    <remove-project name="platform/build" />
    <project path="build/make" name="build" remote="faster" >
        <copyfile dest="Makefile" src="core/root.mk" />
        <linkfile dest="build/CleanSpec.mk" src="CleanSpec.mk" />
        <linkfile dest="build/buildspec.mk.default" src="buildspec.mk.default" />
        <linkfile dest="build/core" src="core" />
        <linkfile dest="build/envsetup.sh" src="envsetup.sh" />
        <linkfile dest="build/target" src="target" />
        <linkfile dest="build/tools" src="tools" />
    </project>

    <remove-project name="platform/frameworks/av" />
    <project path="frameworks/av" name="frameworks_av" remote="faster" />

    <remove-project name="platform/frameworks/base" />
    <project path="frameworks/base" name="frameworks_base" remote="faster" />

    <remove-project name="platform/frameworks/native" />
    <project path="frameworks/native" name="frameworks_native" remote="faster" />

    <remove-project name="platform/frameworks/opt/net/wifi" />
    <project path="frameworks/opt/net/wifi" name="frameworks_opt_net_wifi" remote="faster" />

    <remove-project name="platform/system/core" />
    <project path="system/core" name="system_core" remote="faster" />

    <remove-project name="platform/system/vold" />
    <project path="system/vold" name="system_vold" remote="faster" />

    <remove-project name="platform/system/netd" />
    <project path="system/netd" name="system_netd" remote="faster" />

    <remove-project name="platform/packages/services/Telephony" />
    <project path="packages/services/Telephony" name="packages_services_Telephony" remote="faster" />

    <project path="device/faster/treble" name="device_faster_treble" remote="faster" />
    <project path="vendor/hardware/overlay" name="vendor_hardware_overlay" remote="faster" revision="master" />
</manifest>
